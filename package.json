{
  "name": "@codehelios/verify-tokenid",
  "version": "0.1.1",
  "description": "Library to verify firebase tokenId in Cloudflare Workers for authenticating users using firebase auth tokenId.",
  "type": "module",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.0",
    "@types/jest": "~27.5",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "jest": "~28.1",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  },
  "scripts": {
    "start": "node build/src/index.js",
    "dev": "npm run build && wrangler dev",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch",
    "publish": "npm run build:release && npm publish --access public"
  },
  "author": "Codehelios <97363633+codehelios-com@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codehelios/verify-tokenid.git"
  },
  "dependencies": {
    "tslib": "~2.4"
  },
  "volta": {
    "node": "16.13.0"
  },
  "keywords": [
    "firebase",
    "workers",
    "cloudflare",
    "workers",
    "firebase",
    "tokenId"
  ]
}
